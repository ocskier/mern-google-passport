{
  "name": "yahtzee-online",
  "version": "1.0.0",
  "description": "Full Stack React App with OAuth React and Typescript",
  "main": "./server/server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no tests installed\" && exit 1",
    "prestart": "npm run tsc",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "npm run tsc && NODE_ENV=production node ./server/dist/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start:server": "cd server && nodemon server.js",
    "client": "cd client && npm run start",
    "seed": "node server/scripts/seedDB.js",
    "install": "cd client && npm install",
    "build-ts": "cd client && npm run build",
    "heroku-postbuild": "npm run build-ts",
    "tsc": "",
    "dev": "ts-node-dev --respawn --watch server/**/*.ts --transpileOnly ./server/server.ts"
  },
  "author": "Jon R Jackson",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.4",
    "ts-node-dev": "^1.0.0-pre.62"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express-session": "^1.15.15",
    "@types/mongoose": "^5.5.29",
    "@types/node": "^14.11.1",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.5",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
